<?xml version="1.0"?>
<package format="3">
  <name>humanoid_robot_navigation</name>
  <version>0.3.2</version>
  <description>
    This package is for generating footsteps of ROBOTIS-HUMANOID_ROBOT.
    It has several config files and launch files in order to use footstep planner package. 
  </description>
  <license>Apache 2.0</license>
  <maintainer email="ronaldsonbellande@gmail.com">Ronaldson Bellande</maintainer>


  <buildtool_depend condition="$ROS_VERSION == 1">catkin</buildtool_depend>

  <build_depend condition="$ROS_VERSION == 1">map_server</build_depend>
  <build_depend condition="$ROS_VERSION == 1">octomap_server</build_depend>
  <build_depend condition="$ROS_VERSION == 1">footstep_planner</build_depend>

  <build_export_depend condition="$ROS_VERSION == 1">map_server</build_export_depend>
  <build_export_depend condition="$ROS_VERSION == 1">octomap_server</build_export_depend>
  <build_export_depend condition="$ROS_VERSION == 1">footstep_planner</build_export_depend>

  <exec_depend condition="$ROS_VERSION == 1">map_server</exec_depend>
  <exec_depend condition="$ROS_VERSION == 1">octomap_server</exec_depend>
  <exec_depend condition="$ROS_VERSION == 1">footstep_planner</exec_depend>


  <buildtool_depend condition="$ROS_VERSION == 2">ament_cmake</buildtool_depend>

  <build_depend condition="$ROS_VERSION == 2">map_server</build_depend>
  <build_depend condition="$ROS_VERSION == 2">octomap_server</build_depend>
  <build_depend condition="$ROS_VERSION == 2">footstep_planner</build_depend>

  <build_export_depend condition="$ROS_VERSION == 2">map_server</build_export_depend>
  <build_export_depend condition="$ROS_VERSION == 2">octomap_server</build_export_depend>
  <build_export_depend condition="$ROS_VERSION == 2">footstep_planner</build_export_depend>

  <exec_depend condition="$ROS_VERSION == 2">map_server</exec_depend>
  <exec_depend condition="$ROS_VERSION == 2">octomap_server</exec_depend>
  <exec_depend condition="$ROS_VERSION == 2">footstep_planner</exec_depend>


</package>
