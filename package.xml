<?xml version="1.0"?>
<package format="3">
  <name>humanoid_robot_gazebo</name>
  <version>0.3.2</version>
  <description>
    The humanoid_robot_gazebo package
  </description>
  <license>Apache 2.0</license>
  <maintainer email="ronaldsonbellande@gmail.com">Ronaldson Bellande</maintainer>


  <buildtool_depend condition="$ROS_VERSION == 1">catkin</buildtool_depend>

  <build_depend condition="$ROS_VERSION == 1">controller_manager</build_depend>
  <build_depend condition="$ROS_VERSION == 1">gazebo_ros</build_depend>

  <build_export_depend condition="$ROS_VERSION == 1">controller_manager</build_export_depend>
