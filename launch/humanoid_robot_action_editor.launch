<?xml version="1.0"?>
<launch>
  <param name="gazebo" value="false" type="bool" />
  <param name="gazebo_robot_name" value="humanoid_robot_humanoid" />

  <param name="offset_table" value="$(find humanoid_robot_manager)/config/offset.yaml" />
  <param name="robot_file_path" value="$(find humanoid_robot_manager)/config/HUMANOID_ROBOT.robot" />
  <param name="action_file_path" value="$(find humanoid_robot_action_module)/data/motion_4095.bin" />

  <!-- Action Editor -->
  <node pkg="humanoid_robot_action_editor" type="humanoid_robot_action_editor" name="humanoid_robot_action_editor" output="screen" />

  <!-- sound player -->
  <node pkg="ros_mpg321_player" type="ros_mpg321_player" name="ros_mpg321_player" output="screen" />
</launch>
